  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "@babel/core": "7.14.3",
    "typescript": "4.2.4"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
}
